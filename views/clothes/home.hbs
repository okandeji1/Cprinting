<div class="modal fade" id="detail" tabindex="-1" role="dialog" aria-labelledby="detail" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="detail" v-show="!editmode">Details</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                {{#each productDetail}}
                <ul class="list-unstyled">
                    <li class="list-item">Product category: {{cateory}}{{name}}</li>
                    <li class="list-item">Product Name: {{name}}</li>
                    <li class="list-item">Price: {{price}}</li>
                    <li class="list-item">Description: {{description}}</li>
                </ul>
                {{/each}}
            </div>
        </div>
    </div>
</div>