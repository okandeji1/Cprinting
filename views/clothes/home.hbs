<!-- BEGIN: SEARCH SECTION -->
<div class="row room-check-horizontal">
    <div class="container">
        <form>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <label>PICK UP LOCATION</label>
                <div class="input-group">
                    <input type="text" name="destination" class="form-control" placeholder="E.g Paris" required>
                    <span class="input-group-addon"><i class="fa fa-map-marker fa-fw"></i></span>
                </div>
            </div>
            <div class="col-md-2 col-sm-3 col-xs-6">
                <label>PICK UP DATE</label>
                <div class="input-group">
                    <input type="text" id="check_in" name="check-in" class="form-control" placeholder="DD/MM/YYYY"
                        required>
                    <span class="input-group-addon"><i class="fa fa-calendar fa-fw"></i></span>
                </div>
            </div>
            <div class="col-md-2 col-sm-3 col-xs-6">
                <label>RETURN DATE</label>
                <div class="input-group">
                    <input type="text" id="check_out" name="check-out" class="form-control" placeholder="DD/MM/YYYY"
                        required>
                    <span class="input-group-addon"><i class="fa fa-calendar fa-fw"></i></span>
                </div>
            </div>
            <div class="col-md-1 col-sm-3 col-xs-6">
                <label>BRAND</label>
                <select class="selectpicker" name="car_brand">
                    <option>Audi</option>
                    <option>BMW</option>
                    <option>Mercedes</option>
                </select>
            </div>
            <div class="col-md-1 col-sm-3 col-xs-6">
                <label>TYPE</label>
                <select class="selectpicker" name="car_type">
                    <option>Sedan</option>
                    <option>Van</option>
                    <option>Coupe</option>
                    <option>Lime</option>
                </select>
            </div>
            <div class="clearfix visible-xs-block"></div>
            <div class="col-md-2 text-center check-button">
                <button type="submit">SEARCH</button>
            </div>
        </form>
    </div>
</div>
<!-- END: SEARCH SECTION -->
