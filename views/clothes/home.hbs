<!-- START: PRODUCT SECTION-->
<section class="hotel-product home-product">
    <!-- START: PRODUCT ROW 1 -->
    {{#each products}}
    <div class="row light-row">
        {{#each this}}
        <div class="col-md-6 clear-padding wow slideInLeft">
            <div class="product-wrapper">
                <div class="col-md-6 col-sm-6 home-product-padding tooltip-right">
                    <h4>{{this.category}}{{name}}</h4>
                    <h5><i class="fa fa-certificate"></i> {{this.name}}</h5>
                    <p>{{this.description}}</p>
                    <div class="rating-box">
                        <div class="pull-left">
                        </div>
                        <div class="pull-right">
                            <i class="fa fa-star"></i><i class="fa fa-star"></i><i class="fa fa-star"></i><i
                                class="fa fa-star"></i><i class="fa fa-star-half-o"></i><span>4.5/5</span>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="pricing-info">
                        <div class="pull-left">
                            <span>{{this.price}}</span>
                        </div>
                        <div class="pull-right">
                            <a href="/add-to-cart/{{id}}" class="wow fadeIn add-to-cart">Add To Cart</a>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="col-md-6 col-sm-6 clear-padding image-sm text-center">
                    <img src="/uploads/products/{{this.image}}" alt="cruise">
                    <div class="detail-link-wrapper">
                        <div class="detail-link">
                            <a href="#"><i class="fa fa-search"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    {{/each}}
    <!-- END: PRODUCT ROW 1 -->
</section>
<!-- END: PRODUCT SECTION -->

<!-- BEGIN: TOP DESTINATION -->
<section id="top-tour-row">
    <div class="row top-tour">
        <div class="container">
            <div class="section-title text-center">
                <h2>TOP CLOTHES</h2>
                <h4>CHECK OUT OUR TOP WEARS</h4>
            </div>
            {{#each products}}
            {{#each this}}
            <div class="col-md-4 col-sm-6 tour-grid clear-padding wow slideInUp" data-wow-delay="0.1s">
                <img src="/uploads/products/{{this.image}}" alt="Cruise">
                <div class="tour-brief">
                    <div class="pull-left">
                        <h4><i class="fa fa-certificate"></i>{{this.name}}</h4>
                    </div>
                    <div class="pull-right">
                        <h4>{{this.price}}</h4>
                    </div>
                </div>
                <div class="tour-detail text-center">
                    <p><strong><i class="fa fa-road"></i>Category: </strong> {{this.category}}{{name}}</p>
                    <p><strong><i class="fa fa-clock-o"></i>Name: </strong>{{this.name}}</p>
                    <p><strong><i class="fa fa-clock-o"></i>Description: </strong>{{this.description}}</p>
                    <p><strong><i class="fa fa-clock-o"></i>Price: </strong>{{this.price}}</p>
                    <p><a class="wow fadeIn add-to-cart" href="/detail:{{id}}" data-toggle="modal"
                            data-target="#detail">DETAIL</a>
                    </p>
                </div>
            </div>
            {{/each}}
            {{/each}}
        </div>
    </div>
</section>
<!-- END: TOP DESTINATION -->
